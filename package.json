{
  "name": "arx-predict-express",
  "version": "1.0.0",
  "description": "Express server with TypeScript, Redis, PostgreSQL, Ably chat, and Helius webhook support",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/app.ts",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "ts-node src/prisma/seed.ts"
  },
  "keywords": [
    "express",
    "typescript",
    "redis",
    "postgresql",
    "prisma",
    "ably",
    "helius"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@coral-xyz/anchor": "^0.31.1",
    "@prisma/client": "^5.6.0",
    "@solana/spl-token": "^0.4.13",
    "@solana/web3.js": "^1.98.4",
    "@types/bull": "^3.15.9",
    "ably": "^2.0.0",
    "axios": "^1.6.0",
    "bull": "^4.16.5",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "redis": "^4.6.10"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
